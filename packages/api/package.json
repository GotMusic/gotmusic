  "dependencies": {
    "@tanstack/query-core": "5.52.0",
    "@tanstack/react-query": "^5.62.14",
    "axios": "1.7.7",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "react": "19.0.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "5.6.3"
  }
}
